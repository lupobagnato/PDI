    <sql>SELECT
  ANNO
, MESE
, NROCASO
, IDPAZIENTE
, COD_UO
, DESC_COD_UO
, COD_FISCALE
, Nvl(CRA,'n/d') CRA			-- Codice Regionale Assistito 12.12.2012
, COGNOME
, NOME
, SESSO
, DATANASCITA
, trim(COMUNERESIDENZA) ComuneResidenza			-- trimmato 30.03.2011
, trim(STATORESIDENZA) 	StatoResidenza   		-- aggiunto 17/07/2009, trimmato 30.03.2011
, ASL
, COD_MODALITA_DIMISSIONE
, DATAENTRATA
, ORAENTRATA
, DATADIMISSIONE
, STATO
, trim(CODICE_COLORE_TRIAGE) codice_colore_triage						-- trim colori e codici colori [12/05/2010]
, Nvl(substr(trim(COLORE_TRIAGE), 1,20),'n/d') colore_triage			-- troncati i colori [30/12/2009]
, COD_URGENZA_EXTRA
, nvl(trim(cod_mezzo), '00') COD_MEZZO
, nvl(trim(mezzo), 'n/d') MEZZO
, Trim(COD_MODALITA) cod_modalita
, Trim(MODALITA_ACCESSO) modalita_accesso
, LUOGO_EVENTO
, trim(CODICE_COLORE_VISITA) codice_colore_visita						-- trim colori e codici colori [12/05/2010]
, Nvl(substr(trim(COLORE_VISITA), 1,20),'n/d') colore_visita			-- trim colori e codici colori [12/05/2010]
, nvl(trim(sala_visita), 'n/d') SALA_VISITA
, trim(DATA_VISITA) Data_Visita						-- trim delle date [07/01/2010]
, trim(ORA_VISITA) ora_visita						-- trim delle ore [30/04/2010]
, trim(ORA_DIM) ora_dim								-- trim delle ore [30/04/2010]
, PROGN_GIORNI
, PROGN_RISERVATA
, trim(CODICE_COLORE_DIMISSIONE) codice_colore_dimissione				-- trim colori e codici colori [12/05/2010]
, Nvl(substr(trim(COLORE_DIMISSIONE), 1,20),'n/d') colore_dimissione	-- trim colori e codici colori [12/05/2010]
, ESITO
, RIC_OBI
, CDC
, CDC_APPOGGI  -- Reparto Appoggio [11.01.2016]
, cod_EsitPS
, ESITOBI
, cod_EsitOBI
, nvl(trim(medico), 'n/d') MEDICO
, COD_PRI_DIMI
, COD_PRI_VISI
, COD_PRI_LAST
, importo_ticket ticket_stringa
, to_number(importo_ticket) importo_ticket     -- [09/08/2011] replace con regular expression spostato nella vista

--, to_number(regexp_replace(importo_ticket, '[[:alpha:]]')) importo_ticket   -- RegExp_Replace [08/08/2011]
--, to_number(importo_ticket) importo_ticket   -- 26.8.2010 x ORA-22992, era:Nvl2(importo_ticket, to_number(importo_ticket), Null) importo_ticket
, PROBL_PRINC
, mod_invio		-- 23.04.2020 riutilizzato per "Segni e Sintomi"
, Trim(replace(data_accesso,'.','/')) Data_Accesso			-- trim delle date [07/01/2010]
, trim(ora_accesso) ora_accesso								-- trim delle ore [30/04/2010]
, Substr(Cod_Diagnosi, 1, 5) Cod_Diagnosi
, Substr(Des_Diagnosi, 1, 200) Des_Diagnosi
, nvl(Cod_Tipo_Evento, '00') Cod_Tipo_Evento
, nvl(Tipo_Evento, 'n/d') Tipo_Evento
, Indirizzo													-- Aggiunti 27.07.2011
, Indirizzo_2
, Telefono
, nvl(InitCap(Stato_Paziente), 'n/d') Stato_Paziente
, nvl(Infermiere, 'n/d') Infermiere
, nvl(InitCap(flag_Dolore), 'n/d') flag_Dolore
, nvl(replace(txt_Dolore, 'SCALA E PUNTEGGIO: ',''), 'n/d') txt_Dolore
, Motivo_Trasf
, Istituto_Trasf
, Probl_Triage
FROM V_EXPORT_ACCESSI_PS@patidok p
Where
p.DATADIMISSIONE between Trunc(?) And  Trunc(?) 
order by
datadimissione desc, ora_dim desc
</sql>
