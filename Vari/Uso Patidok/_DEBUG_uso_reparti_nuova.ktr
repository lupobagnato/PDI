    <sql>select 
data_dimissione,
uo_dimissione,
tipo_ricovero,
'new' tipo_cartella,
count(caso) as TOT_DEGENTI,

sum(decode (dati_clinici_preliminari,0,0,1)) as DATI_PRELIMINARI,
sum(decode (scheda_inf_ingresso,0,0,1)) as SCHEDA_INGRESSO,

sum(decode (an_familiare,0,0,1)) as AN_FAMILIARE,
sum(decode (an_fisiologica,0,0,1)) as AN_FISIOLOGICA,
sum(decode (an_patologica,0,0,1)) as AN_PATOLOGICA,
sum(decode (an_prossima,0,0,1)) as AN_PROSSIMA,

sum(decode (es_ob_generale,0,0,1)) as ESAME_OBIETTIVO,
sum(decode (es_obiettivo_spec,0,0,1)) as ESAME_OB_SPEC,

sum(decode (diario_medico,0,0,1)) as DIARIO_CLINICO,
sum(decode (diario_clinico_infermieri,0,0,1)) as DIARIO_INF,

sum(decode (lettera_dimissioni,0,0,1)) as LETTERA_DIMISSIONI,
sum(decode (lettera_dimissioni_def,0,0,1)) as LETTERA_DIMISSIONI_DEF,
sum(decode (lettera_dimissioni_fd,0,0,1)) as LETTERA_DIMISSIONI_FD,

sum(decode (stampa_cartella_clinica,0,0,1)) as CART_CLIN,
sum(decode (stampa_cartella_clinica_def,0,0,1)) as CART_CLIN_DEF,
sum(decode (stampa_cartella_clinica_fd,0,0,1)) as CART_CLIN_FD,

sum(decode (totale_doc_fd,0,0,1)) as DOC_FD

from 
v_cu_uso_reparti@patidok
-- [30.07.2012] la v_cu_uso_reparti_test@patidok e' stata rinominata
where 
data_dimissione between trunc(sysdate - 7) and trunc(sysdate)
group by 
data_dimissione, uo_dimissione, tipo_ricovero

