    <entry>
      <name>Fix_Medico.sql</name>
      <description>SQL</description>
      <type>SQL</type>
      <sql>declare 
  MESE_inizio varCHAR2(6);  
  MESE_fine varCHAR2(6);
  
  i number;
  res number;
  BUFFER_SIZE number := 150000;

  cursor f_import_cur(yyyymm_s varchar2, yyyymm_e varchar2)  is
            select anno, mese, cod_pc
            from f_import
            where
            '20'||anno||mese between yyyymm_s and yyyymm_e
            and filetype = 'S'
            order by 1,2,3;    

begin 
--MESE_inizio := '201802';
select substr(min(r.data_consegna),1,6) into MESE_inizio from ricettari_sogei r
where trunc(r.upd_date) = trunc(sysdate);

MESE_fine   := to_char(sysdate,'yyyymm');
--select substr(max(r.data_consegna),1,6) into MESE_fine from ricettari_sogei r
--where trunc(r.upd_date) = trunc(sysdate);

   for cursore in  f_import_cur(MESE_inizio, MESE_fine) loop
  
             begin
			 dbms_output.enable(BUFFER_SIZE);    
             dbms_output.put_line(cursore.anno || ', ' || cursore.mese || ', ' || cursore.cod_pc) ;
			 dbms_output.disable();    
             res := pkg_spec.fix_medico(cursore.anno, cursore.mese, cursore.cod_pc);
             commit; 
           
             exception
             when others then
                   dbms_output.put_line(cursore.anno || ', ' || cursore.mese || ', ' || cursore.cod_pc || ' : ' || sqlerrm) ;
             rollback;
           end;
  
   end loop;


end; 
</sql>
