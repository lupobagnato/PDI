      <name>__Reset Verif_Date.sql</name>
      <description>SQL</description>
      <type>SQL</type>
      <sql>update 
dwh.f_import f
set
f.verif_date = null
, f.aux_date = sysdate
where
f.verif_date is not null and
'20' || f.anno || f.mese >= -- Least fra MeseScorso e UltimaConsegna
(select Least(to_char(add_months(sysdate, -1),'yyyymm') , substr(max(r.data_consegna),1,6) ) from ricettari_sogei r)
--(select substr(max(least(r.data_consegna, to_char(add_months(sysdate, -1),'yyyymmdd')) ),1,6) from ricettari_sogei r)
and f.load_date &lt;= (select max(ins_date ) from ricettari_sogei )
</sql>
