responseBody.removeNamespace(ws);
responseBody.removeNamespace(soap);

var xmlns = new Namespace("http://ws.cdyne.com/");
responseBody.removeNamespace(xmlns);

// sembra che la maggior parte di queste istruzioni sui namespace
// non cambino nulla di ciò che alla fine si trova in responseBody ... ?

Alert(responseXml + "\n\r" + responseBody.toXMLString());

