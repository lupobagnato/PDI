function stringValue(node) {
    var value = "";
    if (node.hasSimpleContent()) {
        value = node.toString();
    } else {
        for each (var c in node.children()) {
            value += stringValue(c);
        }
    }
    return value;
}

function recursiveDispaly(node) {
    var value = "";
    if (node.hasSimpleContent()) {
        value = node.localName();
    } else {
        for each (var c in node.children()) {
            value += node.localName()+"\n\r\t" + recursiveDispaly(c);
        }
    }
    return value;
}


