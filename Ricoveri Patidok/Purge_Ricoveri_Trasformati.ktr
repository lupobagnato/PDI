    <connection>db_sopra</connection>
    <sql>SELECT
  r.NUM_CASO
, r.IDPAZ_PATIDOK
, r.DATA_ACC
, r.ORA_ACC
, r.DATA_DIM
, r.ORA_DIM
, r.TIPO_RICOV
, r.mot_ricov_dh
, r.DATA_NASCITA
, r.COGNOME
, r.NOME
, r.COD_STATO_NAS
, r.COD_COMUNE_NAS
, r.DES_COMUNE_NAS
, r.COD_COMUNE_RES
, r.COD_COMUNE_DOM
, r.ASL
, r.SESSO
, r.COD_FISC
, r.COD_REP_ACC
, r.DES_REP_ACC
, r.COD_REP_DIM
, r.DES_REP_DIM
, r.DATA_CARICAMENTO
, r.DATA_MODIFICA
, r.FLAGFIELD
, r.JOB_ID
, r.TRANSFORMATION_ID
, r.ANNOMESE_DIM
--- Appoggi in Week Surgery ---
, r.data_ws_in, r.ora_ws_in
, r.data_ws_out, r.ora_ws_out
, r.ore_ws, r.gg_ws
, r.cod_drg
, k.kf_aktiv
, case
	when k.kf_status=6101 then 'AMB in Ka_Fall'
	when ff.ff_status = 7000 then 'Pre-osp'
	else 'Altro non in v_export_ricoveri'
  end Tipo_Annullamento
from 
ricoveri_patidok r
left join v_export_ricoveri@patidok e on e.num_caso = r.num_caso   -- per identificare i casi da cancellare
left join pathpt.ka_fall@patidok k on r.num_caso = k.fall_zahl 	   -- per rilevare quelli diventati ambulatoriali
left join pathpt.fach_fall@patidok ff on (r.num_caso = ff.fall_zahl and ff.ff_status = 7000) -- per rilevare quelli ritrasformati in Pre osp
where
Nvl(r.DATA_DIM, r.DATA_ACC) >= (sysdate - 365*3)  -- dimessi o accettati  negli ultimi 3 anni
and e.num_caso is null

-- r.DATA_ACC Between To_Date('01012009','ddmmyyyy') And To_Date('31122010','ddmmyyyy')
</sql>
