    <entry>
      <name>Imposta Periodo</name>
      <description />
      <type>EVAL</type>
      <script>
inizio = parent_job.getVariable("s_YYYYMMDD");
if (inizio.length() != 8) {									// inizio.length, senza parentesi ritorna nome e tipo della funzione
parent_job.setVariable("s_YYYYMMDD", inizio + "01")
};

fine = parent_job.getVariable("e_YYYYMMDD");
if (fine.length() != 8) {
anno = fine.substr(0,4);
mese_dopo = fine.substr(4,2); // è il mese successivo perché JavaScript li numera partendo da 0
                              // Gennaio è 0, ma è anche 12 viene interpretato come Gennaio

ultimo_g = new Date(anno, mese_dopo, 0).getDate();  // il giorno 0 di un mese è l'ultimo del mese precedente

parent_job.setVariable("e_YYYYMMDD", fine + ultimo_g)
};


true;
</script>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>224</xloc>
